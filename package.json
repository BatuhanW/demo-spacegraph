{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./{src,pages/**/*} --fix --ext .ts,.tsx",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "apollo client:codegen --target typescript --outputFlat",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "deepmerge": "^4.2.2",
    "graphql": "^15.4.0",
    "moment": "^2.29.1",
    "next": "9.5.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "apollo": "^2.31.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.8.13",
    "typescript": "^4.0.3"
  }
}
